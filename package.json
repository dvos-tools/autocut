{
  "name": "autocut",
  "version": "1.0.0",
  "description": "A simple TypeScript program",
  "main": "dist/app.js",
  "scripts": {
    "build": "esbuild src/index.ts --bundle --platform=node --target=node22 --outfile=dist/app.js --sourcemap",
    "start": "node dist/app.js",
    "dev": "ts-node -r tsconfig-paths/register src/index.ts",
    "setup": "./setup-config.sh",
    "service:start": "brew services start autocut",
    "service:stop": "brew services stop autocut",
    "service:restart": "brew services restart autocut",
    "service:status": "brew services list | grep autocut"
  },
  "keywords": [
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "uuid": "^9.0.0",
    "js-yaml": "^4.1.0",
    "node-cron": "^3.0.3"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/uuid": "^10.0.0",
    "@types/js-yaml": "^4.0.0",
    "@types/node-cron": "^3.0.11",
    "esbuild": "^0.19.0",
    "ts-node": "^10.9.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.0"
  }
}
